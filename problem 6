sum_of_squares_list = []   
square_of_sum_list = []
sum_of_squares = 0
square_of_sum = 0
for i in range(1,100):
    square_of_sum_list.append(i)
    sum_of_squares_list.append(i)
for element in square_of_sum_list:
    square_of_sum += element
a = square_of_sum ** 2
print("squares of sum = ",a)
for element in sum_of_squares_list:
    i = element ** 2
    sum_of_squares =+ i
print("sum of squares = ", sum_of_squares)
print("{} - {} = {} ".format(a,sum_of_squares,a - sum_of_squares))
